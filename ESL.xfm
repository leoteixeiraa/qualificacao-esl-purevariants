      <cm:properties>
        
        
        
        
      </cm:properties>
      <cm:vname>
        <cm:mimedesc cm:id="i5ugefhjg-fUJM9Cx" cm:mimetype="text/plain" cm:lang="de" cm:encoding="utf-8">Datentransfer</cm:mimedesc>
        <cm:mimedesc cm:id="iUwGAgRGicjhwRojQ" cm:mimetype="text/plain" cm:encoding="utf-8">PC Data transfer</cm:mimedesc>
      </cm:vname>
      <cm:desc>
        <cm:mimedesc cm:id="iiFZGBNsJgDnRvcTU" cm:mimetype="text/plain" cm:encoding="utf-8">A communication channel to a PC</cm:mimedesc>
        <cm:mimedesc cm:id="i7suOmHJSYRSJ_a4F" cm:mimetype="text/html" cm:encoding="utf-8">&lt;p&gt;This feature provides this communication capability. Using this feature&amp;nbsp;enables the possibility of connecting to a PC and controling the whole weather station&amp;nbsp;from a PC.&amp;nbsp;&lt;/p&gt;&lt;p&gt;Two connection types can be used, USB or RS232. Both interface can be used to connect the weather station directly to a PC.&lt;/p&gt;&lt;p&gt;But these features are alternatives, they can not be used together.&lt;/p&gt;</cm:mimedesc>
      </cm:desc>
    </cm:element>
