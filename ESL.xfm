    <cm:element cm:id="iCyKzd5DGk6-wzau9" cm:name="WeatherMonitor" cm:type="ps:feature" cm:class="ps:feature" cm:default="on">
      <cm:relations cm:id="ivWz-1VLQ6Fu-3eoH" cm:class="ps:children">
