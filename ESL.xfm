      <cm:vname>
        <cm:mimedesc cm:id="ic1krn8fKQ5dYTqJ4" cm:mimetype="text/plain" cm:encoding="utf-8">SyncNGoProto</cm:mimedesc>
      </cm:vname>
      <cm:desc>
        <cm:mimedesc cm:id="iExMLh7Rp69ZfSNE7" cm:mimetype="text/plain" cm:encoding="utf-8">Communicate with simple self-made protocol</cm:mimedesc>
        <cm:mimedesc cm:id="ixZUOT5AWNyZyjFFq" cm:mimetype="text/html" cm:encoding="utf-8">&lt;p&gt;This feature adds our self developed communication protocol.&lt;/p&gt;&lt;p&gt;This protocol is simpler then any standard protocol, so we could optimize it to our weather station needs.&lt;/p&gt;</cm:mimedesc>
      </cm:desc>
    </cm:element>
    <cm:element cm:id="igenT8V_yxVecEhFw" cm:name="UDPoverSLIPProto" cm:type="ps:feature" cm:class="ps:feature">
      <cm:relations cm:id="i9v1mA2di9b0wFuc1" cm:class="ps:parents">
        <cm:relation cm:id="iHcyqz9inCW8vFCH1" cm:type="ps:parent">
          <cm:target cm:id="i7CVHjaJSFGUnZTHE">./iUpDZC6LtRGFzQ8g-</cm:target>
        </cm:relation>
      </cm:relations>
      <cm:relations cm:id="iG7yZv37TZKFMWmjw" cm:class="ps:children">
      </cm:relations>
      <cm:properties>
        
        
        
        
        <cm:property cm:id="iphPMmOvBydkbpMxS" cm:type="ps:string" cm:fixed="true" cm:name="state">
          <cm:constant cm:id="iM7LHOhbNRVPzKMqr" cm:type="ps:string">alpha</cm:constant>
        </cm:property>
      </cm:properties>
      <cm:vname>
        <cm:mimedesc cm:id="iQOE-i1nj8exmiv6t" cm:mimetype="text/plain" cm:encoding="utf-8">UDPoverSLIPProto</cm:mimedesc>
      </cm:vname>
      <cm:desc>
        <cm:mimedesc cm:id="iG5MpUB_RsSb9v3uu" cm:mimetype="text/plain" cm:encoding="utf-8">Communicate with UDP + SLIP protocol</cm:mimedesc>
        <cm:mimedesc cm:id="iy_I5nSW71A_ibIxr" cm:mimetype="text/html" cm:encoding="utf-8">&lt;p&gt;This feature provides a standard communication protocol. It uses a standard User Datagram Protocol.&lt;/p&gt;&lt;p&gt;This is a very simple protocol. This prtocol is used over a Serail Line Internet Protocol. This is a very simple protocol too. But it is not very save. UDP uses checksum for calculating if a message is correct, but it can not correct this message if it is broken. This message has to be resend. And if a message&amp;nbsp;got lost nobody will notice it, cause SLIP has no error detection.&lt;/p&gt;&lt;p&gt;UDP and SLIP have only a small overhead for adressing.&lt;/p&gt;</cm:mimedesc>
      </cm:desc>
    </cm:element>
    <cm:element cm:id="iUpDZC6LtRGFzQ8g-" cm:name="Protocol" cm:type="ps:feature" cm:class="ps:feature" cm:default="on">
